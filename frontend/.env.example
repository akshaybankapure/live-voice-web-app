# LiveKit API Configuration
LIVEKIT_API_KEY=YOUR_API_KEY
LIVEKIT_API_SECRET=YOUR_API_SECRET

# Public configuration - points to local LiveKit server or LiveKit Cloud
NEXT_PUBLIC_LIVEKIT_URL=wss://YOUR_LIVEKIT_URL

# Application Configuration for Voice AI Agent
NEXT_PUBLIC_APP_CONFIG="
title: 'Voice AI Agent'
description: 'Real-time voice AI with Soniox STT, Groq LLM, and ElevenLabs TTS'
github_link: 'https://github.com/livekit/agents-playground'
video_fit: 'contain'
settings:
  editable: true
  theme_color: 'indigo'
  chat: true
  outputs:
    audio: true
    video: false
  inputs:
    mic: true
    camera: false
    sip: false
"
